.social li a.skype
{
    background-position: -42px 0
}

.social li a.yahoo
{
    background-position: -63px 0
}

.social li a.blogger
{
    background-position: -84px 0
}

.clear
{
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
    margin: 0;
    padding: 0
