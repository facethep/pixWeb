#slider
{
    position: relative;
    overflow: hidden;
    margin: 0;
    padding: 0;
    list-style: none
}

#slider li
{
    float: left;
    width: 392px;
    height: 274px;
    overflow: hidden
}

#pager
{
    position: relative;
    overflow: hidden;
    text-align: center;
    margin: 0;
    padding: 12px 0 0;
    list-style: none;
    z-index: 999;
    line-height: 0;
}

#pager li
{
    display: inline-block;
    width: 10px;
    margin: 0 2px
}

#pager li a
{
    display: block;
    width: 10px;
    height: 11px;
}

#pager li.activeSlide a
{
    background-position: 0 0
}

    /* -- 5. SUBSCRIBE FORM -- */

.subscribe
{
    padding-bottom: 21px;
    margin-bottom: 6px
}

.subscribe input
{
    font-family: inherit;
    font-size: 14px;
    color: #87909a;
    width: 250px;
    padding: 11px 10px;
    margin-right: 2px;
    background: #eceff4;
    border-radius: 4px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    -khtml-border-radius: 4px;
    border: 1px solid #e0e4ea;
    -moz-box-shadow: 0 1px 2px rgba(255, 255, 255, 0.3);
    -webkit-box-shadow: 0 2px 2px rgba(255, 255, 255, 0.3);
    box-shadow: 0 2px 2px rgba(255, 255, 255, 0.3);
}

.subscribe input:hover
{
    color: #555f6e;
    border-color: #ccc;
}

.subscribe input:focus
{
    color: #555f6e;
    border-color: #ccc;
    -moz-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1);
    -webkit-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1);
    box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.1);
}

.subscribe input.submit
{
    width: 100px;
    background: url(images/submit_btn.jpg) 0 0 repeat-x;
    outline: none;
    border: none;
    border: 1px solid #e27000;
    color: #fff;
    font-weight: bold;
    text-shadow: 0 1px 0 rgba(0, 0, 0, 0.3);
    cursor: pointer;
    -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);
    -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);
}

.subscribe input.submit:hover
{
    background-position: 0 -45px
}

.subscribe input.submit:active
{
    background-position: 0 -91px;
    color: #703500;
    border: 1px solid #b75700;
    text-shadow: 0 1px 0 rgba(255, 255, 255, 0.2);
    -moz-box-shadow: 0 1px 2px rgba(255, 255, 255, 0.3);
    -webkit-box-shadow: 0 1px 2px rgba(255, 255, 255, 0.3);
    box-shadow: 0 1px 2px rgba(255, 255, 255, 0.3);
}

#success,
#error
{
    display: none;
    color: #666;
    font-size: 11px;
    line-height: 16px;
    padding: 4px 8px;
    background: #d7feba;
    border: 1px solid #c8ecad
}

#error
{
    background: #ffffe0;
    border: 1px solid #e6db55;
    color: #333
}

input.error
{
    border-color: #cc0000
}

